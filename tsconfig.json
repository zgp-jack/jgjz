{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "lib",
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "baseUrl": ".",
    "rootDir": ".",
    "jsx": "preserve",
    "jsxFactory": "Taro.createElement",
    "allowJs": true,
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types",
      "global.d.ts"
    ],
    "paths": {
      "@/hooks/*": [
        "./src/hooks/*"
      ],
      "@/utils/*": [
        "./src/utils/*"
      ],
      "@/models/*": [
        "./src/models/*"
      ],
      "@/config/*": [
        "./src/config/*"
      ],
      "@/images/*": [
        "./src/images/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/assets/*": [
        "./src/assets/*"
      ],
      "@/store/*": [
        "./src/store/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/subpackage/*": [
        "./src/subpackage/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "compileOnSave": false
}
